<h1>Log In</h1>
  <%= form_for(:session, url: login_path) do |f| %>
    <%= render partial: 'shared/errors', locals: {errors: @errors} %>

      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Log In" %>

  <% end %>
